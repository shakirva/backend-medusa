{
  "name": "marqa-souq-backend",
  "version": "0.0.1",
  "description": "Marqa Souq E-Commerce Backend - Built with MedusaJS",
  "author": "Marqa Souq Team",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build && node scripts/apply-branding.js",
    "build:admin": "medusa build && node scripts/apply-branding.js",
    "start": "medusa start",
    "start:dev": "medusa develop",
    "dev": "node scripts/apply-branding.js && medusa develop",
    "dev:branded": "node scripts/apply-branding.js && (node scripts/watch-branding.js &) && medusa develop",
    "watch:branding": "node scripts/watch-branding.js",
    "apply-branding": "node scripts/apply-branding.js",
    "prod:build": "echo 'ðŸ“¦ Building Medusa for production...' && medusa build && node scripts/apply-branding.js && echo 'âœ… Build complete. Next: copy .env to .medusa/server/ and install deps'",
    "prod:install": "cd .medusa/server && yarn install --production",
    "prod:start": "cd .medusa/server && ./node_modules/.bin/medusa start",
    "db:migrate": "npx medusa db:migrate",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "seed:blog": "medusa exec ./src/scripts/seed-blog.ts",
    "publish:products": "medusa exec ./src/scripts/publish-products.ts",
    "create:pubkey": "medusa exec ./src/scripts/create-publishable-key.ts",
    "seed:hero-banner": "medusa exec ./src/scripts/create-sample-hero-banner.ts",
    "create:categories": "medusa exec ./src/scripts/create-categories.ts",
    "create:sample-video": "medusa exec ./src/scripts/create-sample-video.ts",
    "sync:odoo": "medusa exec ./src/scripts/odoo-sync.ts",
    "sync:inventory": "medusa exec ./src/scripts/odoo-inventory-sync.ts",
    "sync:images": "medusa exec ./src/scripts/sync-odoo-images.ts",
    "sync:orders": "medusa exec ./src/scripts/sync-odoo-orders.ts",
    "sync:customers": "medusa exec ./src/scripts/sync-odoo-customers.ts",
    "sync:all": "yarn sync:odoo && yarn sync:images && yarn sync:customers",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.10.3",
    "@medusajs/cli": "2.10.3",
    "@medusajs/framework": "2.10.3",
    "@medusajs/medusa": "2.10.3",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "awilix": "^8.0.1",
    "axios": "^1.13.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.0",
    "sqlite3": "^5.1.7",
    "xmlrpc": "^1.3.2"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.3",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/busboy": "^1.5.4",
    "@types/jest": "^29.5.13",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "@types/xmlrpc": "^1.3.10",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}