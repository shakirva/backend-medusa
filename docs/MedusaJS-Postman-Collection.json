{
  "info": {
    "name": "MedusaJS API Collection",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Storefront APIs",
      "item": [
        {
          "name": "List Products",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/products"
          }
        },
        {
          "name": "Get Product Details",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/products/:id",
            "description": "Replace :id with the product ID"
          }
        },
        {
          "name": "List Categories",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/categories"
          }
        },
        {
          "name": "List Collections",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/collections"
          }
        },
        {
          "name": "Create Cart",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/store/carts"
          }
        },
        {
          "name": "Get Cart Details",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/carts/:id",
            "description": "Replace :id with the cart ID"
          }
        },
        {
          "name": "Add Item to Cart",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/store/carts/:id/line-items",
            "description": "Replace :id with the cart ID"
          }
        },
        {
          "name": "Remove Item from Cart",
          "request": {
            "method": "DELETE",
            "url": "{{base_url}}/store/carts/:id/line-items/:item",
            "description": "Replace :id with the cart ID and :item with the item ID"
          }
        },
        {
          "name": "Complete Checkout",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/store/carts/:id/complete",
            "description": "Replace :id with the cart ID"
          }
        },
        {
          "name": "List Orders",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/orders"
          }
        },
        {
          "name": "Get Order Details",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/orders/:id",
            "description": "Replace :id with the order ID"
          }
        },
        {
          "name": "Register Customer",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/store/customers"
          }
        },
        {
          "name": "Customer Login",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/store/auth"
          }
        },
        {
          "name": "Customer Logout",
          "request": {
            "method": "DELETE",
            "url": "{{base_url}}/store/auth"
          }
        }
      ]
    },
    {
      "name": "Admin APIs",
      "item": [
        {
          "name": "Manage Products",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/admin/products"
          }
        },
        {
          "name": "Manage Categories",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/admin/categories"
          }
        },
        {
          "name": "Manage Collections",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/admin/collections"
          }
        },
        {
          "name": "Manage Orders",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/admin/orders"
          }
        },
        {
          "name": "Manage Customers",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/admin/customers"
          }
        },
        {
          "name": "Manage Discounts",
          "request": {
            "method": "GET",
