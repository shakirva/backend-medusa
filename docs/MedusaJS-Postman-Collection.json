    {
      "name": "Brands API",
      "item": [
        {
          "name": "List All Brands",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/brands"
          }
        },
        {
          "name": "Get Brand Details",
          "request": {
            "method": "GET",
            "url": "{{base_url}}/store/brands/:id",
            "description": "Replace :id with the brand ID"
          }
        },
        {
          "name": "Create Brand",
          "request": {
            "method": "POST",
            "url": "{{base_url}}/admin/brands",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Brand Name\",\n  \"description\": \"Brand Description\",\n  \"logo_url\": \"https://example.com/logo.png\"\n}"
            },
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ]
          }
        },
        {
          "name": "Update Brand",
          "request": {
            "method": "PUT",
            "url": "{{base_url}}/admin/brands/:id",
            "description": "Replace :id with the brand ID",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Updated Brand Name\",\n  \"description\": \"Updated Description\",\n  \"logo_url\": \"https://example.com/new-logo.png\"\n}"
            },
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ]
          }
        },
        {
          "name": "Delete Brand",
          "request": {
            "method": "DELETE",
            "url": "{{base_url}}/admin/brands/:id",
            "description": "Replace :id with the brand ID"
          }
        }
      ]
    }
  ]
}